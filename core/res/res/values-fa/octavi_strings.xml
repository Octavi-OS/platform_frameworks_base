    <string name="pocket_lock_message_long_press">نگه داشتن دکمه پاور برای تعامل</string>
    <!-- Gaming mode -->
    <string name="notification_channel_gaming">حالت مخصوص بازی کردن</string>
    <string name="gaming_mode_notification_title">حالت بازی فعال شد</string>
    <string name="gaming_mode_notification_content">برای غیرفعال کردن حالت بازی ضربه بزنید</string>
