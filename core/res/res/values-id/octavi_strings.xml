    <!-- MicroG -->
    <string name="permlab_fakePackageSignature">Palsukan tanda tangan paket</string>
    <!-- Description of an application permission, listed so the user can choose whether
         they want to allow the application to do this. -->
    <string name="permdesc_fakePackageSignature">Mengizinkan aplikasi berpura-pura menjadi aplikasi lain. Aplikasi jahat mungkin dapat menggunakan ini untuk mengakses data aplikasi pribadi. Penggunaan yang sah termasuk emulator yang berpura-pura menjadi emulatornya. Berikan izin ini dengan hati-hati saja!</string>
    <!-- Title of a category of application permissions, listed so the user can choose whether they want to allow the application to do this. -->
    <string name="permgrouplab_fake_package_signature">Palsukan tanda tangan paket</string>
    <!-- Description of a category of application permissions, listed so the user can choose whether they want to allow the application to do this. -->
    <string name="permgroupdesc_fake_package_signature">izinkan untuk memalsukan tanda tangan paket</string>
    <!-- Message shown to the user when the apps requests permission from this group. If ever possible this should stay below 80 characters (assuming the parameters takes 20 characters). Don't abbreviate until the message reaches 120 characters though. [CHAR LIMIT=120] -->
    <string name="permgrouprequest_fake_package_signature">Izinkan
        &lt;b&gt;<xliff:g id="app_name" example="Gmail">%1$s</xliff:g>&lt;/b&gt; untuk memalsukan tanda tangan paket?</string>
    <!-- Vendor mismatch fingerprint warning [CHAR LIMIT=NONE] -->
    <string name="system_error_vendorprint">Gambar vendor Anda tidak sesuai dengan sistem. Silakan flash gambar vendor <xliff:g id="string">%s</xliff:g> untuk perangkat Anda</string>
    <!-- Aerr strings -->
    <string name="aerr_disable">Nonaktifkan aplikasi</string>
