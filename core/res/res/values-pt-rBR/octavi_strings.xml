    <string name="url_copy_success">URL copiada com sucesso</string>
    <string name="url_copy_failed">Ocorreu um erro durante o upload do log para o dogbin</string>
    <!-- Pocket Lock -->
    <string name="pocket_lock_message_long_press">Manter pressionado o botão liga/desliga para interagir</string>
    <!-- Gaming mode -->
    <string name="notification_channel_gaming">Modo de jogo</string>
    <string name="gaming_mode_notification_title">Modo de jogo ativado</string>
    <string name="gaming_mode_notification_content">Toque para desativar o modo de jogo</string>
