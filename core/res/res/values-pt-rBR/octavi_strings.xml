    <string name="gaming_mode_disabled_toast">O modo de jogo foi desativado</string>
    <!-- MicroG -->
    <string name="permlab_fakePackageSignature">Assinatura falsa de pacote</string>
    <!-- Description of an application permission, listed so the user can choose whether
         they want to allow the application to do this. -->
    <string name="permdesc_fakePackageSignature">Permite que o aplicativo finja ser um aplicativo diferente. Aplicativos maliciosos podem usar isso para acessar dados de aplicativos privados. Usos legítimos incluem um emulador fingindo ser o que ele emula. Conceda esta permissão com cautela apenas!</string>
    <!-- Title of a category of application permissions, listed so the user can choose whether they want to allow the application to do this. -->
    <string name="permgrouplab_fake_package_signature">Assinatura falsa de pacote</string>
    <!-- Description of a category of application permissions, listed so the user can choose whether they want to allow the application to do this. -->
    <string name="permgroupdesc_fake_package_signature">permitir falsificação de assinatura de pacote</string>
    <!-- Message shown to the user when the apps requests permission from this group. If ever possible this should stay below 80 characters (assuming the parameters takes 20 characters). Don't abbreviate until the message reaches 120 characters though. [CHAR LIMIT=120] -->
    <string name="permgrouprequest_fake_package_signature">Permitir que
        &lt;b&gt;<xliff:g id="app_name" example="Gmail">%1$s</xliff:g>&lt;/b&gt; falsifique a assinatura do pacote?</string>
    <!-- Vendor mismatch fingerprint warning [CHAR LIMIT=NONE] -->
    <string name="system_error_vendorprint">Sua imagem de fornecedor não coincide com o sistema. Por favor instale a <xliff:g id="string">%s</xliff:g> imagem de fornecedor para o seu dispositivo</string>
    <!-- Aerr strings -->
    <string name="aerr_disable">Desativar aplicativo</string>
</resources>
